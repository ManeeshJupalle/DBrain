	<div id="searchbox" class="d-none header__search-content Coveostate CoveoComponentState CoveoComponentOptions CoveoQueryController CoveoDebug">
		<div class="CoveoAnalytics" data-search-hub="GlobalHub"></div>
		<div id="mainHeaderSearchBox" class="CoveoSearchbox" data-enable-omnibox="true" data-enable-query-suggest-addon="true" data-query-suggest-character-threshold="1" data-placeholder="Search All Ansys"></div>
		<input type="hidden" id="searchResultsPath" name="searchResultsPath" value="/en-in/search"/>
		<input type="hidden" id="coveoApiKey" name="coveoApiKey" value="xxef7fe774-c402-403c-9c60-c62a18507557"/>
		<input type="hidden" id="locale" name="locale" value="en-in"/>
	</div>
	
	
    

	
	
	<script>
		var thecoveoscriptloaded, searchTextBeforeLoad;
		$(document).ready(function() {
			if($("#pseudosearchbox")){
				searchTextBeforeLoad=$("#pseudosearchbox").find(":input[name='q']").val();
			}

			$(".header__menu-content .CoveoSearchbox input").attr("placeholder", $(".header__menu-content .CoveoSearchbox").attr("data-placeholder"));
			setTimeout(function() {
				if (thecoveoscriptloaded != true) {										
					$("#pseudosearchbox").find(":input[name='q']").val(searchTextBeforeLoad);
					$("body").one("click", function(e) {
						$.when(
						$.getScript("https://static.cloud.coveo.com/searchui/v2.10090/3/js/CoveoJsSearch.Lazy.min.js"),
						$.Deferred(function(deferred) {
							$(deferred.resolve);
						})
						).done(function(script, textStatus) {
							/*$.getScript("https://static.cloud.coveo.com/searchui/v2.10090/3/js/templates/templates.js");*/
							if ($(e.target).closest('div[id]').attr("id") == "mainHeaderSearchBox") {
								var theheadersearchclicked = true;
							} else {
								var theheadersearchclicked = false;
							}
							if ($(e.target).closest('span[class]').attr("class") == "coveo-search-button") {
								var theheadersearchiconclicked = true;
							} else {
								var theheadersearchiconclicked = false;
							}
							
							const getCookieValue = (name) => (
							document.cookie.match('(^|;)\\s*' + name + '\\s*=\\s*([^;]+)')?.pop() || ''
							)
							var $apiKey = $('[id=coveoApiKey]');
							var coveoCookieValue = getCookieValue('coveoApiKey');
							if(coveoCookieValue === '') {
								$.ajax({
									type: 'GET',
									url: '/content/ansysincprogram/en-us/home/search.coveotoken.json?v=1.0',
									success: function(msg) {
										$apiKey.val(msg['token']);
										document.cookie = 'coveoApiKey=' + msg['token'] + '; max-age=' + 24*60*60*.95 + ";path=/";
									}
								});
							} else {
								$apiKey.val(coveoCookieValue);
							}
							
							var apiKey = $('#coveoApiKey').val();
							var locale = $('#locale').val();
							
							const searchBoxRoot = document.getElementById("searchbox");
							Coveo.SearchEndpoint.configureCloudV2Endpoint("", apiKey);
							
							var searchResultsPath = $('#searchResultsPath').val();
							Coveo.initSearchbox(searchBoxRoot, searchResultsPath);
							// Use Coveo Query Builder to set locale
							Coveo.$$(searchBoxRoot).on('buildingQuerySuggest', function(e, args) {
								args.payload.tab = "AllAnsysTab"
								args.payload.locale = locale.split('-')[0];
							});
							Coveo.$$(searchBoxRoot).on("afterInitialization", function(e, args) {
								//add keyword from pseudoserachbox to search box before it loads
								$("#searchbox").find(":input").val(searchTextBeforeLoad); 
								var searchBoxDiv = Coveo.$$(document).find("#mainHeaderSearchBox > .CoveoOmnibox");
								if (searchBoxDiv) {
									searchBoxDiv.setAttribute("aria-label", "Search Suggestions");
								}
							});
							
							Coveo.$$(searchBoxRoot).on('doneBuildingQuery', function(e, args) {
								const IsEmptyOrWhiteSpace = (str) => (str.match(/^\s*$/) || []).length > 0;
								var queryInput = args.queryBuilder.expression.getParts()[0];//returns the query the user has entered in the search box
								if ( !queryInput ) {
									args.cancel = true;
									console.log('search did not initiate. the query is empty.');
								}
								else if ( IsEmptyOrWhiteSpace(queryInput) ) {
									args.cancel = true;
									console.log('search did not initiate. the query cannot be empty spaces.');
								}
							});
							
							var checkSelected =  setInterval(function(){ 
								var check_selected = $(".header__search-content").hasClass("coveo-after-initialization");
								if(check_selected) {
									//fetch the value from pseudoSearchBox before it gets truncated
									searchTextBeforeLoad=$("#pseudosearchbox").find(":input[name='q']").val();
									$("#searchbox").find(":input").val(searchTextBeforeLoad); 
									$("#pseudosearchbox").hide().remove();
									$("#searchbox").addClass("CoveSearchInterface").removeClass("d-none");
									if (theheadersearchclicked && theheadersearchiconclicked) {
										$(".header__search-content .coveo-search-button").click();
									}
									if (theheadersearchclicked) {
										
										$(".header__search-content .magic-box-input input[type=text]").focus();
										$(".header__search-content .magic-box-input input[type=text]").select();
									}
									clearInterval(checkSelected);
								}
							}, 500);
						});
					});
				} else {
					$(".header__search-content .CoveoOmnibox.coveo-query-syntax-disabled.magic-box, .header__search-content .CoveoSearchButton.coveo-accessible-button").hide();
					var apiKey = $('#coveoApiKey').val();
					var locale = $('#locale').val();

					const searchBoxRoot = document.getElementById("searchbox");
					Coveo.SearchEndpoint.configureCloudV2Endpoint("", apiKey);
					
					var searchResultsPath = $('#searchResultsPath').val();
					Coveo.initSearchbox(searchBoxRoot, searchResultsPath);
					// Use Coveo Query Builder to set locale
					Coveo.$$(searchBoxRoot).on('buildingQuerySuggest', function(e, args) {
						args.payload.tab = "AllAnsysTab"
						args.payload.locale = locale.split('-')[0];
					});

					Coveo.$$(searchBoxRoot).on("afterInitialization", function(e, args) {
						var searchBoxDiv = Coveo.$$(document).find("#mainHeaderSearchBox > .CoveoOmnibox");
						if (searchBoxDiv) {
							searchBoxDiv.setAttribute("aria-label", "Search Suggestions");
						}
					});

					//fetch the value from pseudoSearchBox before it gets truncated
					$("#searchbox").find(":input").val(searchTextBeforeLoad); 
					$("#pseudosearchbox").hide().remove();
					$("#searchbox").removeClass("d-none");
	
					Coveo.$$(searchBoxRoot).on('doneBuildingQuery', function(e, args) {
						const IsEmptyOrWhiteSpace = (str) => (str.match(/^\s*$/) || []).length > 0;
						var queryInput = args.queryBuilder.expression.getParts()[0];//returns the query the user has entered in the search box
						if ( !queryInput ) {
							args.cancel = true;
							console.log('search did not initiate. the query is empty.');
						}
						else if ( IsEmptyOrWhiteSpace(queryInput) ) {
							args.cancel = true;
							console.log('search did not initiate. the query cannot be empty spaces.');
						}
					});
				}
			},50);
			
		});
	</script>
        </div>
    </div>
</header></div>

        
    </div>

</div>
</div>

    
